# Render Build Configuration
# This file tells Render to disable caching for this project

# Disable build cache
RENDER_BUILD_CACHE=false

# Force fresh builds
RENDER_FRESH_BUILD=true

# Clear cache on every deployment
CLEAR_CACHE_ON_DEPLOY=true

